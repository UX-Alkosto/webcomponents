{
	"env": {
		"browser": true,
		"es2021": true,
		"es6": true
	},
	"extends": ["eslint:recommended", "prettier"],
	"plugins": ["import"],
	"globals": {
		"setJavaPresent": true
	},
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"rules": {
		"indent": ["error", 4],
		"linebreak-style": ["error", "unix"],
		"quotes": ["error", "single"],
		"semi": ["error", "always"]
	}
}
